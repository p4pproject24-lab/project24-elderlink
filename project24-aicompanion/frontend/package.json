    "@types/react": "~19.0.10",
    "@types/sockjs-client": "^1.5.4",
    "@types/testing-library__jest-dom": "^5.14.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
