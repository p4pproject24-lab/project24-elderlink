{
  "name": "project24",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^react-native$": "react-native",
      "^.+\\.(svg|png|jpg|jpeg)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/types.ts",
      "!src/types/**",
      "!src/**/index.ts",
      "!src/hooks/useUpdateUserProfile.ts",
      "!src/hooks/useTTS.ts",
      "!src/hooks/useHeyGen.ts",
      "!src/components/ElderlySelectorModal.tsx",
      "!src/components/DailySummaries.tsx",
      "!src/navigation/AppNavigator.tsx",
      "!src/navigation/RootNavigator.tsx",
      "!src/theme/typography.ts",
      "!src/screens/**",
      "!App.tsx"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@livekit/react-native": "^2.7.6",
    "@livekit/react-native-webrtc": "^137.0.1",
